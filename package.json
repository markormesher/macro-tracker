    "express-session": "^1.15.6",
    "history": "^4.7.2",
    "logform": "^2.0.0",
    "moment": "^2.20.1",
    "passport": "^0.4.0",
    "passport-google-oauth": "^1.0.0",
    "pg": "^7.4.3",
    "qs": "^6.5.2",
    "quagga": "^0.12.1",
    "react": "^16.4.2",
    "react-chartjs-2": "^2.7.6",
    "react-dom": "^16.4.2",
    "react-loadable": "^5.5.0",
    "react-redux": "^6.0.0",
