{
  "name": "macro-tracker",
  "sideEffects": false,
  "version": "0.4.1",
  "license": "Apache-2.0",
  "scripts": {
    "// root commands": "//",
    "prod": "run-s dc-build-prod dc-up-prod",
    "dev": "run-s dc-build-dev dc-up-dev dc-down",
    "test": "run-s clean check-cross-refs build-test start-test",
    "// docker-compose shortcuts": "//",
    "dc-build-prod": "docker-compose build",
    "dc-build-dev": "docker-compose -f docker-compose.yml -f docker-compose.dev.yml build",
    "dc-up-prod": "docker-compose up",
    "dc-up-dev": "docker-compose -f docker-compose.yml -f docker-compose.dev.yml up",
    "dc-down": "docker-compose down",
    "// entry points": "//",
    "start-api": "cross-env NODE_ENV=production node ./build/api/server.js",
    "start-api-dev": "cross-env NODE_ENV=development node ./build/api/server.js",
    "start-client": "cross-env NODE_ENV=production node ./build/client/server.js",
    "start-client-dev": "cross-env NODE_ENV=development node ./build/client/server.js",
    "start-test": "cross-env NODE_ENV=test nyc mocha -r ./src/test-utils/jsdom-setup.js \"./build/**/*.js\"",
    "// root build jobs": "//",
    "build-api": "run-s build-api-tsc copy-api-config",
    "build-api-dev": "run-s build-api-dev-tsc copy-api-config",
    "build-api-test": "run-s build-api-test-tsc copy-api-config",
    "build-client": "run-s build-client-tsc build-client-webpack",
    "build-client-dev": "run-s build-client-dev-tsc build-client-dev-webpack",
    "build-client-test": "run-s build-client-test-webpack",
    "build-test": "run-p build-api-test build-client-test",
    "// inner build jobs": "//",
    "build-api-tsc": "tsc -p ./tsconfig.api.json",
    "build-api-dev-tsc": "tsc -p ./tsconfig.api.json",
    "build-api-test-tsc": "tsc -p ./tsconfig.api-test.json",
    "build-client-tsc": "tsc -p ./tsconfig.client.json",
    "build-client-dev-tsc": "tsc -p ./tsconfig.client.json",
    "build-client-webpack": "cross-env NODE_ENV=production webpack -p",
    "build-client-dev-webpack": "cross-env NODE_ENV=development webpack",
    "build-client-test-webpack": "cross-env NODE_ENV=test webpack",
    "// linters": "//",
    "lint": "run-p lint-ts lint-scss",
    "lint-scss": "sass-lint -v -c ./sasslint.yml \"./src/**/*.scss\"",
    "lint-ts": "tslint -c ./tslint.json \"./src/**/*.{ts,tsx}\"",
    "lint-ts-fix": "tslint --fix -c ./tslint.json \"./src/**/*.{ts,tsx}\"",
    "// misc": "//",
    "clean": "rimraf ./build ./coverage ./.nyc_output",
    "copy-api-config": "copyfiles -u 2 -e \"./**/*.ts\" \"./src/api/config/**\" ./build/api/.",
    "check-cross-refs": "./.scripts/check-cross-project-references.sh"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.6",
    "@fortawesome/free-brands-svg-icons": "^5.8.1",
    "@fortawesome/pro-light-svg-icons": "^5.8.1",
